issn = {1364503X},
journal = {Philosophical Transactions of the Royal Society A: Mathematical, Physical and Engineering Sciences},
keywords = {Adaptive cruise control,Car-following model,Dynamic capacity,Free-flow capacity,Human driving behaviour,Microscopic traffic simulation},
number = {1928},
pages = {4585--4605},
title = {{Enhanced intelligent driver model to access the impact of driving strategies on traffic capacity}},
