journal = {IEEE Access},
keywords = {Trajectory planning,autonomous driving,collision checking,kinodynamic constraints,motion planning,obstacle avoidance},
pages = {32800--32819},
publisher = {IEEE},
title = {{Hybrid Trajectory Planning for Autonomous Driving in Highly Constrained Environments}},
volume = {6},
year = {2018}
}
