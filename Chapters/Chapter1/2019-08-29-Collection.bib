doi = {10.1109/TITS.2018.2867256},
file = {:C$\backslash$:/Users/Ed/OneDrive - University of Leeds/Literature Review/Cooperative Motion of Road Vehicles/Belkhouche 2018 - Collaboration and Optimal Conflict Resolution at an Unsignalized Intersection.pdf:pdf},
