pages = {346--359},
title = {{Optimal trajectories for time-critical street scenarios using discretized terminal manifolds}},
volume = {31},
year = {2012}
}

@phdthesis{Lundberg2017,
abstract = {Autonomous vehicles is a rapidly expanding eld and the need for robust and ecient path planners is high. We approach the global pathplanning problem for an autonomous load carrier for quarry environments, developed at Volvo Construction Equipment, using two methods: a twostep path planning and path smoothing approach, and a method based on an optimal control formulation of the path planning problem. The two-step method is based on smoothing an initial path found by A*, an ecient grid search algorithm, by tting a curve consisting of as few clothoid segments as possible to the A* path. The smoothing is done by rewriting the non-linear optimization problem to a convex form by a linearization of the deviation constraints around the curvature of the A* path. An iterative method is then used to relax the `0-norm, which measures the number of non-zero elements in a vector, with a weighted `1-norm which, in turn, is then solved eciently using CVX in Matlab. The optimal control based path planning method solves the nonlinear optimization problem using IPOPT. It was found that the completeness of the A* algorithm, coupled with the guaranteed solution of a convex problem, resulted in a very robust method that was able to nd paths through mazes and dicult situations. The optimal control approach produced better paths, but had a tendency to sometimes show inconsistent behavior.},
author = {Lundberg, Marcus},
file = {:C$\backslash$:/Users/tsedl/OneDrive - University of Leeds/Literature Review/Path Planning/Clothoids/Lundberg 2017 degree project.pdf:pdf},
mendeley-groups = {Planning/Route Planning,VEHITS 2019 Submission,Planning/Clothoids},
school = {KTH Royal Institute of Technology},
title = {{Path planning for autonomous vehicles using clothoid based smoothing of A * generated paths and optimal control}},
year = {2017}
}

