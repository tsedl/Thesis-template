journal = {Proceedings - 13th IEEE International Conference on Ubiquitous Intelligence and Computing, 13th IEEE International Conference on Advanced and Trusted Computing, 16th IEEE International Conference on Scalable Computing and Communications, IEEE Internationa},
keywords = {Autonomous intersection control,Convex optimization,Quality of experience,Vehicular networks},
mendeley-groups = {Cooperative Motion of Road Vehicles/Intersections},
pages = {203--210},
publisher = {IEEE},
title = {{A Convex Optimization Based Autonomous Intersection Control Strategy in Vehicular Cyber-Physical Systems}},
year = {2017}
