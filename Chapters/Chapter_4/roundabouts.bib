journal = {IEEE Transactions on Intelligent Transportation Systems},
keywords = {Connected and automated vehicles (CAVs),cooperative driving,intersection control,merging highways,vehicle coordination,vehicle-to-infrastructure (V2I) communication,vehicle-to-vehicle (V2V) communication},
