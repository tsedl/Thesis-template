issn = {24058963},
journal = {IFAC-PapersOnLine},
keywords = {Connected,automated vehicles,cooperative merging control,roundabouts,vehicle coordination},
mendeley-groups = {Cooperative Motion of Road Vehicles/Roundabouts},
number = {9},
pages = {73--78},
publisher = {Elsevier B.V.},
title = {{Optimal Control of Connected and Automated Vehicles at Roundabouts: An Investigation in a Mixed-Traffic Environment}},
url = {https://doi.org/10.1016/j.ifacol.2018.07.013},
volume = {51},
year = {2018}
}
